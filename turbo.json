{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "stream",
  "globalDependencies": ["turbo.json", "pnpm-workspace.yaml", "vitest.config.ts"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./dist/**"]
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": []
    }
  }
}
