{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "npm.packageManager": "pnpm",
  "npm.scriptRunner": "pnpm",
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.associations": {
    "api-extractor.json": "jsonc"
  },
  "files.eol": "\n",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.insertFinalNewline": true,
  "github.copilot.chat.commitMessageGeneration.instructions": [
    {
      "file": "node_modules/@map-colonies/infra-copilot-instructions/instructions/commit.md"
    }
  ],
  "json.schemas": [
    {
      "fileMatch": ["packages/semantic-conventions/src/**/*.json"],
      "url": "./packages/semantic-conventions/schemas/attribute.schema.json"
    }
  ],
  "files.associations": {
    "packages/semantic-conventions/src/*.json": "jsonc"
  }
}
